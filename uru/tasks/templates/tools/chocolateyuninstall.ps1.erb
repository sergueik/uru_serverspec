try {
  $chocoBinDir = Join-Path $env:ChocolateyInstall 'bin'

  $uruWrapperBat = Join-Path $chocoBinDir "<%= data[:name] %>.bat"
  $uruWrapperPs1 = Join-Path $chocoBinDir "<%= data[:name] %>.ps1"

  Write-Host "---> Deleting .bat and .ps1 uru wrapper scripts from $chocoBinDir"
  if (Test-Path ($uruWrapperBat)) { Remove-Item $uruWrapperBat -force }
  if (Test-Path ($uruWrapperPs1)) { Remove-Item $uruWrapperPs1 -force }

} catch {
  Write-Host "---> error while uninstalling uru"
  throw $_.Exception
}
